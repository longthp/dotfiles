set shell powershell
set hidden true
set ignorecase true
set dirpreviews false
set icons true
set scrolloff 10

cmd open &start $Env:f

map e $nvim $Env:f
map w $powershell
map a %~/dotfiles/lf/test.ps1

map p paste
map x cut
map y copy
map R reload
map C clear

map <enter> shell
map <tab> $~/scoop/apps/quicklook/current/QuickLook.exe $Env:f
map mp $~/scoop/apps/mpv/current/mpv.exe $Env:f
